<div class="container col-md-5">

<%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name), class:'container2' ) do |f| %>
  <%= f.error_notification %>
  <div class="bd1">
    <div class="form-row">
      <div class="form-group col-md-4">
        ID
        <%=  f.input_field :userid, required: true ,autofocus: true, class:'form-control'%>
      </div>
      <div class="form-group col-md-4">
        Password
        <%= f.password_field :password, class: 'form-control', required: true, hint: ("#{@minimum_password_length} characters minimum" if @minimum_password_length) %>      
      </div>
      <div class="form-group col-md-4">
        Password(one more)
        <%= f.password_field :password_confirmation, required: true ,class:'form-control'%>
      </div>
    </div>
    
    <div class="form-group">
      City
      <%= f.input_field :city, required: true ,class:'form-control'%>
    </div>
    <div class="form-group">
      Adress
    <%= f.input_field :address, required: true ,class:'form-control'%>
    </div>
    <div class="form-row">
      <div class="form-group col-md-6">
        Phomenumber
        <%= f.input_field :phoneNumber, required: true, class:'form-control'%>
      </div>
    </div>
     <div class="b1">
       <%= f.button :submit, "Sign up" ,class:'btn btn-primary'%>
       
     </div>
   </div> 
    

<% end %>

<%= render "devise/shared/links" %>

</div>